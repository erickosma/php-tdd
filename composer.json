{

    "name": "erickosma/php-tdd",
    "description": "Teste TDD-PHP.",
    "keywords": ["TDD", "PHP", "TDD-PHP", "teste php"],
    "license": "MIT",
    "type": "project",
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
